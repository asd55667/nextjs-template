  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint . --fix",
    "lint:biome": "biome lint . --fix",
    "format": "biome format --write .",
    "typecheck": "contentlayer2 build && tsc --noEmit",
    "build:docs": "contentlayer2 build",
    "build:registry": "tsx --tsconfig ./tsconfig.scripts.json ./src/scripts/build-registry.mts",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --config=playwright.ci.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "prepare": "node ./scripts/install-husky.js",
    "release": "changeset version"
  },

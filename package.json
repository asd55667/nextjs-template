    "postcss-preset-env": "^10.1.3",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-pretty-code": "^0.6.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.3",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^6.0.1",
    "shiki": "^1.26.1",
    "tailwindcss": "^3.4.17",
    "ts-morph": "^25.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unified": "^11.0.5",
    "unist-builder": "^3.0.0",
    "unist-util-visit": "^4.1.2"
  },
  "volta": {
    "node": "22.12.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "biome format --write --no-errors-on-unmatched"
    ],
    "**/*.{json,yaml,yml}": [
      "biome format --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "stackblitz": {
    "startCommand": "pnpm install && pnpm run build:registry && pnpm run dev",
    "env": {
      "STACKBLITZ": "true"
    }
  }
}

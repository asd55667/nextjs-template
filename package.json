    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
